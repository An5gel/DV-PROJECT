doctype html
head
  meta(charset='UTF-8')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  link(rel='stylesheet' href='../set/bootstrap/css')
  link(rel='stylesheet' href='../css/signup.css')
  title Document
.signup-form
  form
    .mb-3
      label.form-label(for='exampleInputEmail1') First Name
      input#exampleInputEmail1.form-control(type='email' aria-describedby='emailHelp')
    .mb-3
      label.form-label(for='exampleInputPassword1') Last Name
      input#exampleInputPassword1.form-control(type='password')
    .mb-3
      label.form-label(for='exampleInputEmail1') Email address
      input#exampleInputEmail1.form-control(type='email' aria-describedby='emailHelp')
      #emailHelp.form-text We&apos;ll never share your email with anyone else.
    .mb-3
      label.form-label(for='exampleInputPassword1') Password
      input#exampleInputPassword1.form-control(type='password')
    .mb-3
      label.form-label(for='exampleInputPassword1')  Confirm Password
      input#exampleInputPassword1.form-control(type='password')
    .mb-3.form-check
      input#exampleCheck1.form-check-input(type='checkbox')
      label.form-check-label(for='exampleCheck1') Check me out
    button.btn.btn-primary(type='button' onclick="location.href='register-page.html'") Create Account
script(src='../set/bootstrap/js')
